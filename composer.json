{
  "name": "biller/bcu",
  "description": "Adaptador de WebServices del BCU para PHP",
  "license": "MIT",
  "minimum-stability": "stable",
  "type": "library",
  "require": {
    "php": "^8.2",
    "ext-soap": "*"
  },
  "require-dev": {
